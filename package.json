  "scripts": {
    "babel-build": "babel src -d dist --extensions \".js,.jsx,.ts,.tsx\"",
    "build": "yarn clean-dist && yarn && yarn type-check && yarn babel-build && yarn generate-type-declarations && yarn copy-package-json",
    "clean-dist": "rm -rf dist",
    "copy-package-json": "cp package.json dist/package.json",
    "generate-type-declarations": "tsc --project tsconfig.json --allowJs false --declaration --declarationDir \"dist\" --emitDeclarationOnly --rootDir \"src\"",
    "type-check": "tsc --project tsconfig.json --noEmit"
  },
