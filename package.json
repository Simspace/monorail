{
  "name": "monorail",
  "version": "0.0.0",
  "private": true,
  "description": "Monorail 3",
  "repository": "https://gitlab.com/simspace/monorail",
  "license": "Apache-2.0",
  "author": "SimSpace",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn org:build",
    "build-pack": "ultra -r build-pack",
    "build-storybook": "yarn workspace @monorail/storybook build-storybook --output-dir storybook",
    "build-playroom": "NODE_OPTIONS=\"--max-old-space-size=8192\" yarn playroom build",
    "clean": "ultra -r clean",
    "eslint": "ultra -r eslint",
    "org:build": "tsc -b ./tsconfig.json --verbose",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:eslint": "cd $INIT_CWD && eslint",
    "org:jest": "cd $INIT_CWD && jest",
    "org:monorail-pack": "cd $INIT_CWD && monorail-pack",
    "org:tsc": "cd $INIT_CWD && tsc",
    "prettier:check": "ultra -r prettier:check",
    "prettier:write": "ultra -r prettier:write",
    "storybook": "yarn workspace @monorail/storybook storybook",
    "start-playroom": "yarn playroom start",
    "test": "ultra -r test"
  },
