    "@eslint/compat": "^1.2.4",
    "@fncts/codegen": "^0.0.22",
    "@simspace/config-eslint": "^1.0.5",
    "@simspace/config-prettier": "^1.0.1",
    "@simspace/config-typescript": "^1.0.1",
    "assert": "^2.0.0",
    "babel-jest": "27.5.1",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "concurrently": "^8.1.0",
    "eslint": "9.17.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-codegen": "^0.29.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jest": "29.3.0",
    "jest-axe": "5.0.1",
    "jest-config": "29.3.0",
    "lint-staged": "13.2.2",
    "playroom": "^0.31.0",
    "prettier": "3.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ts-morph": "18.0.0",
    "ts-node": "^10.9.1",
    "ts-pattern": "^5.5.0",
