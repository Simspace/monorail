  "scripts": {
    "build": "concurrently \"yarn build-esm\" \"yarn build-cjs\"",
    "build:pack": "yarn build && yarn ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" scripts/pack.ts",
    "build-esm": "tsc -b tsconfig.build-esm.json",
    "build-cjs": "tsc -b tsconfig.build-cjs.json",
    "test": "jest",
    "clean": "rm -rf dist/",
    "eslint": "NODE_OPTIONS=\"--max-old-space-size=8192\" eslint \"src/**/*.{js,ts,tsx}\"",
    "prettier:check": "prettier './src/**/*{.js,.ts,.tsx}' --list-different",
    "prettier:write": "prettier './src/**/*{.js,.ts,.tsx}' --write",
    "storybook": "start-storybook -p 6006 -s assets",
    "storybook:ci": "start-storybook --ci -p 6006 -s assets",
    "build-storybook": "build-storybook -s assets",
    "test:visual": "start-server-and-test 'yarn storybook' 6006 'yarn creevey --ui'",
    "test:visual:ci": "start-server-and-test 'yarn storybook:ci' 6006 'yarn creevey'",
    "ci": "npm-run-all --parallel eslint test",
    "nix-post-build": "yarn build",
    "dev": "yarn build --watch"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix-type layout --fix --cache",
      "jest --findRelatedTests"
    ]
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "clsx": "^1.2.1",
    "react-glider": "^3.1.0"
  }
