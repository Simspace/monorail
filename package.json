{
  "name": "monorail",
  "version": "0.0.0",
  "private": true,
  "description": "Monorail 3",
  "repository": "https://gitlab.com/simspace/monorail",
  "license": "Apache-2.0",
  "author": "SimSpace",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn org:build",
    "build-pack": "ultra -r build-pack",
    "build-storybook": "yarn workspace @monorail/storybook build-storybook --output-dir storybook",
    "build-playroom": "NODE_OPTIONS=\"--max-old-space-size=8192\" yarn playroom build",
    "clean": "ultra -r clean",
    "eslint": "ultra -r eslint",
    "org:build": "tsc -b ./tsconfig.json --verbose",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:eslint": "cd $INIT_CWD && eslint",
    "org:jest": "cd $INIT_CWD && jest",
    "org:monorail-pack": "cd $INIT_CWD && monorail-pack",
    "org:tsc": "cd $INIT_CWD && tsc",
    "prettier:check": "ultra -r prettier:check",
    "prettier:write": "ultra -r prettier:write",
    "storybook": "yarn workspace @monorail/storybook storybook",
    "start-playroom": "yarn playroom start",
    "test": "ultra -r test"
  },
  "resolutions": {
    "@storybook/client-api": "6.5.10",
    "@types/react": "17.0.39",
    "@types/react-syntax-highlighter": "^11.0.5",
    "react-syntax-highlighter": "^13.5.3",
    "webpack": "5.74.0",
    "@mui/x-data-grid@6.0.1": "patch:@mui/x-data-grid@npm:6.0.1#.yarn/patches/@mui-x-data-grid-npm-6.0.1-9c2a21edb1.patch",
    "@mui/x-data-grid-pro@6.0.1": "patch:@mui/x-data-grid-pro@npm:6.0.1#.yarn/patches/@mui-x-data-grid-pro-npm-6.0.1-e148cf68ba.patch",
    "@mui/x-data-grid-premium@6.0.1": "patch:@mui/x-data-grid-premium@npm:6.0.1#.yarn/patches/@mui-x-data-grid-premium-npm-6.0.1-cf68b61705.patch",
    "@mui/x-date-pickers@6.0.1": "patch:@mui/x-date-pickers@npm:6.0.1#.yarn/patches/@mui-x-date-pickers-npm-6.0.1-46efb3859e.patch",
    "@mui/x-date-pickers-pro@6.0.1": "patch:@mui/x-date-pickers-pro@npm:6.0.1#.yarn/patches/@mui-x-date-pickers-pro-npm-6.0.1-6844cc2dac.patch",
