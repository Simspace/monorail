{
  "name": "monorail",
  "version": "0.0.0",
  "private": true,
  "description": "Monorail 3",
  "repository": "https://gitlab.com/simspace/monorail",
  "license": "Apache-2.0",
  "author": "SimSpace",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn org:build",
    "build-pack": "yarn workspaces -Atp  run build-pack",
    "build-storybook": "yarn workspace @monorail/storybook build-storybook --output-dir storybook",
    "build-playroom": "NODE_OPTIONS=\"--max-old-space-size=8192\" yarn playroom build",
    "clean": "yarn workspaces foreach -Ap run clean",
    "lint:check": "yarn workspaces foreach -Ap run lint:check",
    "lint": "yarn workspaces foreach -Ap run lint",
    "format:check": "yarn workspaces foreach -Ap run prettier:check",
    "format": "yarn workspaces foreach -Ap run prettier:write",
    "storybook": "yarn workspace @monorail/storybook storybook",
    "start-playroom": "yarn playroom start",
    "test": "yarn workspaces foreach -Ap run test",
    "org:build": "tsc -b ./tsconfig.json --verbose",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:eslint": "cd $INIT_CWD && eslint",
    "org:jest": "cd $INIT_CWD && jest",
    "org:monorail-pack": "cd $INIT_CWD && monorail-pack",
    "org:tsc": "cd $INIT_CWD && tsc"
  },
  "resolutions": {
    "@babel/core": "^7.22.1",
    "@types/react": "18.3.12",
    "@types/react-syntax-highlighter": "^11.0.5",
    "react-syntax-highlighter": "^13.5.3",
    "@mui/system": "6.1.8",
    "@mui/x-tree-view@^7.7.0": "patch:@mui/x-tree-view@npm%3A7.7.0#./.yarn/patches/@mui-x-tree-view-npm-7.7.0-3b8d68b815.patch",
    "@mui/x-date-pickers@7.7.0": "patch:@mui/x-date-pickers@npm%3A7.7.0#./.yarn/patches/@mui-x-date-pickers-npm-7.7.0-28f1eebe70.patch",
    "@mui/x-date-pickers-pro@7.7.0": "patch:@mui/x-date-pickers-pro@npm%3A7.7.0#./.yarn/patches/@mui-x-date-pickers-pro-npm-7.7.0-11abe36339.patch",
    "@mui/lab@5.0.0-alpha.170": "patch:@mui/lab@npm%3A5.0.0-alpha.170#./.yarn/patches/@mui-lab-npm-5.0.0-alpha.153-d6d65ebdd5.patch",
    "@mui/x-data-grid@7.7.0": "patch:@mui/x-data-grid@npm%3A7.7.0#./.yarn/patches/@mui-x-data-grid-npm-7.7.0-00c88be7af.patch",
    "@mui/x-data-grid-premium@7.7.0": "patch:@mui/x-data-grid-premium@npm%3A7.7.0#./.yarn/patches/@mui-x-data-grid-premium-npm-6.18.1-aeb54fa10a.patch",
    "@mui/x-data-grid-pro@7.7.0": "patch:@mui/x-data-grid-pro@npm%3A7.7.0#./.yarn/patches/@mui-x-data-grid-pro-npm-6.18.1-96c92648e4.patch"
  },
  "devDependencies": {
    "@0x706b/eslint-plugin-module-specifier-extensions": "^0.2.0",
