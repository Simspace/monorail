{
  "name": "monorail",
  "version": "0.0.0",
  "private": true,
  "description": "Monorail 3",
  "repository": "https://gitlab.com/simspace/monorail",
  "license": "Apache-2.0",
  "author": "SimSpace",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn org:build",
    "build-pack": "ultra -r build-pack",
    "build-storybook": "yarn workspace @monorail/storybook build-storybook --output-dir storybook",
    "build-playroom": "NODE_OPTIONS=\"--max-old-space-size=8192\" yarn playroom build",
    "clean": "ultra -r clean",
    "eslint": "ultra -r eslint",
    "org:build": "tsc -b ./tsconfig.json --verbose",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:eslint": "cd $INIT_CWD && eslint",
    "org:jest": "cd $INIT_CWD && jest",
    "org:monorail-pack": "cd $INIT_CWD && monorail-pack",
    "org:tsc": "cd $INIT_CWD && tsc",
    "prettier:check": "ultra -r prettier:check",
    "prettier:write": "ultra -r prettier:write",
    "storybook": "yarn workspace @monorail/storybook storybook",
    "start-playroom": "yarn playroom start",
    "test": "ultra -r test"
  },
  "resolutions": {
    "@mui/lab": "patch:@mui/lab@npm:5.0.0-alpha.101#.yarn/patches/@mui-lab-npm-5.0.0-alpha.92-3f8a0aeb7b.patch",
    "@mui/x-data-grid-pro": "patch:@mui/x-data-grid-pro@npm:5.17.4#.yarn/patches/@mui-x-data-grid-pro-npm-5.15.0-a4d9069608.patch",
    "@mui/x-data-grid": "patch:@mui/x-data-grid@npm:5.17.4#.yarn/patches/@mui-x-data-grid-npm-5.15.0-be98809963.patch",
    "@mui/x-date-pickers@5.0.3": "patch:@mui/x-date-pickers@npm:5.0.3#.yarn/patches/@mui-x-date-pickers-npm-5.0.3-d0ecb11394.patch",
    "@mui/x-date-pickers-pro@5.0.3": "patch:@mui/x-date-pickers-pro@npm:5.0.3#.yarn/patches/@mui-x-date-pickers-pro-npm-5.0.3-d84c84758c.patch",
    "@storybook/client-api": "6.5.10",
    "@types/react": "17.0.39",
    "@types/react-syntax-highlighter": "^11.0.5",
    "react-syntax-highlighter": "^13.5.3",
    "webpack": "5.74.0"
  },
  "devDependencies": {
    "@0x706b/eslint-plugin-module-specifier-extensions": "0.1.7",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@fncts/codegen": "0.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/experimental-utils": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "assert": "^2.0.0",
    "babel-jest": "27.5.1",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "concurrently": "^7.4.0",
    "eslint": "8.24.0",
    "eslint-plugin-codegen": "^0.16.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "27.5.1",
    "jest-axe": "5.0.1",
    "jest-config": "27.5.1",
    "lint-staged": "10.5.4",
    "playroom": "^0.28.0",
    "prettier": "2.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-morph": "11.0.3",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4",
    "ultra-runner": "^3.10.5"
  },
  "packageManager": "yarn@3.2.1"
}
