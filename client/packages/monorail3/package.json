{
  "name": "@simspace/monorail3",
  "version": "0.0.0",
  "description": "Monorail 3",
  "repository": "https://github.com/Simspace/portal-suite",
  "author": "SimSpace",
  "license": "UNLICENSED",
  "peerDependencies": {
    "@material-ui/core": "5.0.0-beta.0",
    "@material-ui/icons": "5.0.0-beta.0",
    "@material-ui/styled-engine-sc": "5.0.0-beta.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@material-ui/core": "5.0.0-beta.0",
    "@material-ui/icons": "5.0.0-beta.0",
    "@material-ui/styled-engine-sc": "5.0.0-beta.0",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-controls": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "barrelsby": "2.2.0",
    "jest": "27.0.4",
    "jest-axe": "5.0.1",
    "react": "17.0.2",
    "react-docgen-typescript": "2.0.0",
    "react-dom": "17.0.2",
    "styled-components": "5.3.0",
    "ts-morph": "11.0.0",
    "typescript": "4.3.2"
  },
  "resolutions": {},
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "yarn generate-barrel",
    "build": "tsc",
    "postbuild": "yarn clean-barrel",
    "test": "jest",
    "clean": "rm -rf dist/ node_modules/ && yarn clean-barrel",
    "prettier": "../../node_modules/.bin/prettier './src/**/*{.ts,.tsx}' --write",
    "generate-barrel": "barrelsby --directory ./src --exclude '(__tests__)|(__stories__)|(.gen.tsx)' --delete && yarn prettier",
    "clean-barrel": "rm src/index.ts",
    "gen": "./tools/gen",
    "clean-gen": "find ./src/components -name '*.gen.*' -print | xargs rm && yarn gen",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "testRegex": "^.+(jest|test)\\.(ts|tsx)$",
    "setupFiles": [
      "<rootDir>/jest/jest.setup.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/jest.postsetup.ts"
    ],
    "moduleNameMapper": {
      "@material-ui/styled-engine": "@material-ui/styled-engine-sc"
    },
    "testEnvironment": "jsdom"
  }
}
