{
  "name": "@monorail/storybook",
  "version": "0.0.22",
  "private": true,
  "description": "Monorail 3 Storybook",
  "repository": "https://gitlab.com/simspace/monorail",
  "license": "UNLICENSED",
  "author": "SimSpace",
  "scripts": {
    "build": "tsc -b ./tsconfig.json",
    "build-storybook": "TS_NODE_PROJECT=./.storybook/tsconfig.json build-storybook -s assets",
    "clean": "rm -rf build",
    "eslint": "NODE_OPTIONS=\"--max-old-space-size=8192\" yarn org:eslint \"src/**/*.{js,ts,tsx}\"",
    "prettier:check": "prettier './src/**/*{.js,.ts,.tsx}' --list-different",
    "prettier:write": "prettier './src/**/*{.js,.ts,.tsx}' --write",
    "storybook": "TS_NODE_PROJECT=./.storybook/tsconfig.json start-storybook --debug-webpack -p 6006 -s assets",
    "test": "yarn org:jest"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@monorail/components": "workspace:*",
    "@monorail/themes": "workspace:*",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-controls": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/core": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@testing-library/dom": "7.31.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "@types/react-window": "^1.8.5",
    "creevey": "0.7.39",
    "enhanced-resolve": "^5.10.0",
    "lorem-ipsum": "^2.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-window": "^1.8.7",
    "webpack": "^5.74.0"
  }
}
