{
  "name": "@monorail/storybook",
  "version": "0.0.85",
  "private": true,
  "description": "Monorail 3 Storybook",
  "repository": "https://gitlab.com/simspace/monorail",
  "license": "UNLICENSED",
  "author": "SimSpace",
  "scripts": {
    "build": "tsc -b ./tsconfig.json",
    "build-storybook": "storybook build -s assets",
    "clean": "rm -rf build",
    "eslint": "NODE_OPTIONS=\"--max-old-space-size=8192\" yarn org:eslint \"src/**/*.{js,ts,tsx}\"",
    "prettier:check": "prettier './src/**/*{.js,.ts,.tsx}' --list-different",
    "prettier:write": "prettier './src/**/*{.js,.ts,.tsx}' --write",
    "storybook": "storybook dev --debug-webpack -p 6006 -s assets",
    "test": "yarn org:jest --maxWorkers=2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-typescript": "^7.21.5",
    "@monorail/components": "workspace:*",
    "@monorail/themes": "workspace:*",
    "@storybook/addon-a11y": "^7.0.20",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-controls": "^7.0.20",
    "@storybook/addon-docs": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/core": "^6.5.16",
    "@storybook/node-logger": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-webpack5": "^7.0.20",
    "@testing-library/dom": "7.31.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-window": "^1.8.5",
    "creevey": "0.7.39",
    "enhanced-resolve": "^5.14.1",
    "jest-environment-jsdom": "^29.3.0",
    "lorem-ipsum": "^2.0.8",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-window": "^1.8.9",
    "storybook": "^7.0.20",
    "webpack": "^5.86.0"
  }
}
